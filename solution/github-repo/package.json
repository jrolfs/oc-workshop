{
  "name": "github-repo",
  "description": "This is a component for showing the most starred repository for a given user",
  "version": "1.0.0",
  "repository": "https://github.com/matteofigus/oc-workshop/tree/master/solution/github-repo",
  "author": "Matteo Figus <matteofigus@gmail.com>",
  "dependencies": {
    "request": ""
  },
  "oc": {
    "files": {
      "data": "server.js",
      "template": {
        "src": "template.jade",
        "type": "jade"
      }
    },
    "parameters": {
      "username": {
        "description": "Github username",
        "type": "string",
        "mandatory": true,
        "example": "matteofigus"
      }
    }
  }
}
